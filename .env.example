# Example Environment Variables for Riven (backend only!)

#-------------------------------------
# Non settings file variables
#-------------------------------------

# This is used to force the use of the env variables all the time!
# By default, this is false, which only new setting files will be created
# using the env variables you specified.
RIVEN_FORCE_ENV=false

# This is used to specify settings filename. By default it's settings.json.
SETTINGS_FILENAME="settings.json"

# This will reset the database and recreate all tables, and then exit after running!
HARD_RESET=false

# Manual api key, must be 32 characters long
API_KEY=1234567890qwertyuiopas

# Custom trakt client id used by trakt indexer
TRAKT_API_CLIENT_ID=

#-------------------------------------
# Riven Settings
#-------------------------------------

RIVEN_VERSION=0.14.1
RIVEN_DEBUG=true
RIVEN_LOG=true
RIVEN_FORCE_REFRESH=false
RIVEN_MAP_METADATA=true
RIVEN_TRACEMALLOC=false

#-------------------------------------
# Filesystem (VFS) Settings
#-------------------------------------
# Symlink settings have been removed. Configure the Filesystem/VFS settings in your settings file.

# VFS host mount path (where Riven mounts the FUSE filesystem on the host)
RIVEN_FILESYSTEM_MOUNT_PATH=.data/mount
# VFS library path inside containers/clients (what Plex/players see, must map to the mount)
RIVEN_FILESYSTEM_LIBRARY_PATH=/mount
# Enable extra FUSE debug logs from RivenVFS
RIVEN_FILESYSTEM_DEBUG_FUSE=false
# Place anime under separate top-level dirs (/anime_movies, /anime_shows)
RIVEN_FILESYSTEM_SEPARATE_ANIME_DIRS=false

# VFS Performance Settings
# Readahead buffer size in MB (4-64MB recommended, higher for 4K content)
RIVEN_FILESYSTEM_READAHEAD_BUFFER_MB=4
# URL cache TTL in minutes (how long to cache provider URLs)
RIVEN_FILESYSTEM_URL_CACHE_TTL_MINUTES=15

# HTTP Request Settings
# HTTP request timeout in seconds
RIVEN_FILESYSTEM_HTTP_TIMEOUT_SECONDS=30
# HTTP connection timeout in seconds
RIVEN_FILESYSTEM_HTTP_CONNECT_TIMEOUT_SECONDS=5
# Minimum transfer speed in KB/s before timeout
RIVEN_FILESYSTEM_HTTP_LOW_SPEED_LIMIT_KBPS=10
# Time in seconds to maintain minimum speed
RIVEN_FILESYSTEM_HTTP_LOW_SPEED_TIME_SECONDS=15

# FUSE Cache Settings
# FUSE entry cache timeout in seconds
RIVEN_FILESYSTEM_FUSE_ENTRY_TIMEOUT_SECONDS=300
# FUSE attribute cache timeout in seconds
RIVEN_FILESYSTEM_FUSE_ATTR_TIMEOUT_SECONDS=300

# Advanced Settings
# Serialize HTTP requests per file path (disable for better concurrency)
RIVEN_FILESYSTEM_ENABLE_REQUEST_SERIALIZATION=true
# Enable HTTP keep-alive connections
RIVEN_FILESYSTEM_ENABLE_HTTP_KEEPALIVE=true
# Maximum concurrent HTTP requests per file
RIVEN_FILESYSTEM_MAX_CONCURRENT_REQUESTS_PER_FILE=1

# Retry and Error Handling
# Maximum HTTP request retry attempts
RIVEN_FILESYSTEM_HTTP_MAX_RETRIES=2
# Delay between retry attempts in seconds
RIVEN_FILESYSTEM_RETRY_DELAY_SECONDS=1.0
# Use exponential backoff for retries
RIVEN_FILESYSTEM_ENABLE_EXPONENTIAL_BACKOFF=true

# FUSE Mount Options
# Allow other users to access the mount
RIVEN_FILESYSTEM_ENABLE_ALLOW_OTHER=true
# Automatically unmount on process exit
RIVEN_FILESYSTEM_ENABLE_AUTO_UNMOUNT=true
# Maximum background FUSE requests
RIVEN_FILESYSTEM_FUSE_MAX_BACKGROUND=12
# FUSE congestion threshold
RIVEN_FILESYSTEM_FUSE_CONGESTION_THRESHOLD=10

# Buffer Management
# Dynamically adjust buffer size based on bitrate
RIVEN_FILESYSTEM_ENABLE_ADAPTIVE_BUFFERING=false
# Minimum buffer size in MB for adaptive buffering
RIVEN_FILESYSTEM_MIN_BUFFER_MB=1
# Maximum buffer size in MB for adaptive buffering
RIVEN_FILESYSTEM_MAX_BUFFER_MB=128
# Prefetch multiplier for sequential reads
RIVEN_FILESYSTEM_BUFFER_PREFETCH_FACTOR=1.5

# Connection Pool Settings
# Maximum HTTP connections per host
RIVEN_FILESYSTEM_MAX_CONNECTIONS_PER_HOST=10
# Connection pool timeout in seconds
RIVEN_FILESYSTEM_CONNECTION_POOL_TIMEOUT_SECONDS=60
# Enable HTTP/2 support (experimental)
RIVEN_FILESYSTEM_ENABLE_HTTP2=false

# Monitoring and Logging
# Enable VFS performance metrics collection
RIVEN_FILESYSTEM_ENABLE_PERFORMANCE_METRICS=false
# Log requests slower than this threshold (seconds)
RIVEN_FILESYSTEM_LOG_SLOW_REQUESTS_THRESHOLD_SECONDS=5.0
# Monitor and log bandwidth usage
RIVEN_FILESYSTEM_ENABLE_BANDWIDTH_MONITORING=false

# File Handle Management
# Maximum number of open file handles
RIVEN_FILESYSTEM_MAX_OPEN_FILES=1000
# Timeout for idle file handles (seconds)
RIVEN_FILESYSTEM_FILE_HANDLE_TIMEOUT_SECONDS=300
# Reuse file handles when possible
RIVEN_FILESYSTEM_ENABLE_FILE_HANDLE_POOLING=true

